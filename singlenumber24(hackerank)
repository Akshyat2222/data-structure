#include <stdio.h>

int singleNumber(int nums[], int n) {
    int result = 0;
    for (int i = 0; i < n; i++) {
        result ^= nums[i]; 
    }
    return result;
}

int main() {
    int n;
    scanf("%d", &n); 
    int nums[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]); 
    }
    int single = singleNumber(nums, n);
    printf("%d\n", single); 
    return 0;
}
