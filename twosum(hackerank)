#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int asd(int arr[],int n,int a){
    int sum ;
    for(int i = 0; i<n; i++){
        for(int j = i+1; j<n; j++){
           sum = arr[i]+arr[j];
            if(sum == a){
                printf("%d %d", i,j );
            }
        }
        
    }
      return 0;
    
}

int main() {
            int n,a;
            scanf("%d", &n);
            int arr[n];
            for(int i = 0; i<n; i++){
                scanf("%d", &arr[i]);
                
            }
         scanf("%d", &a);
    asd(arr,n,a);
    
            

    return 0;
}
